<?xml version='1.0' encoding='utf-8'?>
<widget id="cieslakmatty.predictionsweb" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>PredictionsWeb</name>
    <description>
        A PredictionsWeb AngularJS implmentation.
    </description>
    <author email="cieslakmatty@gmail.com" href="#">
        Mateusz Cieslak
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="windows-target-version" value="10.0" />
    <engine name="android" spec="^6.3.0" />
    <engine name="browser" spec="^5.0.0" />
    <engine name="windows" spec="^5.0.0" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.2" />
</widget>
